{
  "plugins": ["prettier-plugin-tailwindcss"],
  "semi": true,
  "trailingComma": "all",
  "singleQuote": true,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "trimTrailingWhitespace": true,
  "bracketSameLine": false,
  "htmlWhitespaceSensitivity": "strict",
  "proseWrap": "preserve"
}